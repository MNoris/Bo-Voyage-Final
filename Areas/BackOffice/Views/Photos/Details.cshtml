@model Bo_Voyage_Final.Models.Photo

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Photo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NomFichier)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomFichier)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdDestinationNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdDestinationNavigation.Nom)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-outline-secondary">Modifier</a>
    <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-outline-danger">Supprimer</a>
    <a asp-action="EditerVoyages" class="btn btn-outline-info">Retour à la liste</a>
</div>
